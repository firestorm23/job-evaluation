<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity repository-class="SiteBundle\Repository\PhotoRepository" name="SiteBundle\Entity\Photo">
        <id name="id" type="integer" column="id">
          <generator strategy="AUTO"/>
        </id>
        <field name="name" type="string" column="name" length="1024"/>
        <field name="originalName" type="string" column="originalName" length="1024"/>
        <field name="dirname" type="string" column="dirname" length="1024"/>
        <field name="extension" type="string" column="extension" length="255"/>
        <field name="mimetype" type="string" column="mimetype" length="512"/>
    </entity>
</doctrine-mapping>